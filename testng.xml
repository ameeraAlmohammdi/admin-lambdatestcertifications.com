<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="LambdaTest Parallel Suite" parallel="tests" thread-count="2">
	<test name="Windows 10 Chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="ameera.CertificationTests.SimpleFormTest" />
			<class name="ameera.CertificationTests.LambdaTestInputFormTest" />
			<class name="ameera.CertificationTests.LambdaTestSliderTest" />
		</classes>
	</test>

	<test name="macOS Catalina Safari">
		<parameter name="browser" value="safari" />
		<classes>
			<class name="ameera.CertificationTests.SimpleFormTest" />
			<class name="ameera.CertificationTests.LambdaTestInputFormTest" />
			<class name="ameera.CertificationTests.LambdaTestSliderTest" />
		</classes>
	</test>
</suite>